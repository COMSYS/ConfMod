    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: FastAPI App",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "cwd": "${workspaceFolder}${pathSeparator}backend${pathSeparator}app",
            "args": [
                "main:app",
                "--reload"
            ],
            "jinja": true
        },
        {
            "name": "Python Debugger: app/main.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}${pathSeparator}backend${pathSeparator}run.py",
            "cwd": "${workspaceFolder}${pathSeparator}backend",
            "console": "integratedTerminal"
        }
    ]
}